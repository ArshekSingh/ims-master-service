+                case "BR": {
+                    areaMasterList = areaMasterRepository.findAllByOrgIdAndAreaType(userSession.getCompany().getCompanyId(), "CL");
+                    return new Response(SUCCESS, entityToDtoList(areaMasterList), HttpStatus.OK);
+                }
